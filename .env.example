# Site Configuration
NEXT_PUBLIC_SITE_NAME="GYC Skills Pipeline"

# Admin Password (for /admin route)
# Use ADMIN_PASSWORD (server-side only) for better security
# Or NEXT_PUBLIC_ADMIN_PASSWORD if you need client-side access (less secure)
ADMIN_PASSWORD="your-secure-password-here"
# NEXT_PUBLIC_ADMIN_PASSWORD="your-secure-password-here"

# Supabase Configuration
# Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key-here"

# Optional: Supabase Service Role Key (for admin operations)
# Only use this server-side, never expose to client
# SUPABASE_SERVICE_ROLE_KEY="your-service-role-key-here"

